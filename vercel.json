{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/gerar_conta",
      "dest": "/app.py"
    },
    {
      "src": "/monitorar_emails/(.*)",
      "dest": "/app.py"
    }
  ]
}
